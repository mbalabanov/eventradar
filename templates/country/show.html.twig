{% extends 'base.html.twig' %}

{% block title %}Country{% endblock %}

{% block body %}
    <h1>Country</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Countryid</th>
                <td>{{ country.countryid }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ country.name }}</td>
            </tr>
            <tr>
                <th>Flagurl</th>
                <td>{{ country.flagurl }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('country_index') }}">back to list</a>

    <a href="{{ path('country_edit', {'countryid': country.countryid}) }}">edit</a>

    {{ include('country/_delete_form.html.twig') }}
{% endblock %}
